{
  "name": "frontend-rubyscore",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS=\"--max_old_space_size=4096\" next dev ",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "prettier": "npx prettier \"{app,public,src}/**/*.ts\" --write",
    "format": "yarn prettier && yarn lint:fix"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.8",
    "@mui/system": "^5.14.9",
    "@rainbow-me/rainbowkit": "^1.0.11",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.3.0",
    "@types/pluralize": "^0.0.30",
    "@types/swiper": "^6.0.0",
    "@types/uuid": "^9.0.4",
    "add": "^2.0.6",
    "encoding": "^0.1.13",
    "ethers": "^6.7.1",
    "lokijs": "^1.5.12",
    "modern-normalize": "^2.0.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "pino-pretty": "^10.2.0",
    "pluralize": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "^2.1.4",
    "styled-components": "^6.0.8",
    "swiper": "^9.2.4",
    "uuid": "^9.0.1",
    "viem": "^1.10.9",
    "wagmi": "^1.4.1",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "5.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json}": [
      "npm run format"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": false,
    "useTabs": false,
    "printWidth": 110,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
